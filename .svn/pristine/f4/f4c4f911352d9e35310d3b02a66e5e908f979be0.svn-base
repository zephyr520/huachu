  <select id="queryList" parameterType="com.huachu.dto.request.query.QueryRecyclingListReqDTO" resultType="com.huachu.dto.response.RecyclingListRespDTO">
  	select
  		r1.recycling_no, 
  		r1.file_no, 
  		r1.plate_no, 
  		r1.car_model, 
  		r1.organization,
  		r1.recycling_status,
  		r1.recycling_time,
  		r1.storage_status, 
  		r1.storage_time, 
  		r1.create_time,
  		u1.real_name as creator,
  		s1.repair_shop_name,
  		s1.repair_shop_addr,
  		s1.line, 
  		s2.staff_name,
  		s2.staff_phone,
  		r2.expire_date,
  		r2.dispatch_time,
  		u2.real_name as recycling_user_name,
  		u3.real_name as dispatch_user_name
  	from t_recycling_list r1
  	left join t_recycling_dispatch r2 on r1.dispatch_id=r2.id
  	left join t_repair_shop s1 on r1.repair_shop_id=s1.id
  	left join t_staff s2 on r1.staff_id=s2.id
  	left join t_admin_user u1 on r1.create_id=u1.user_id
  	left join t_admin_user u2 on r2.recycling_user_id=u2.user_id
  	left join t_admin_user u3 on r2.dispatch_user_id=u3.user_id
  	<where>
  		<if test="recyclingUserId != null and recyclingUserId != 0">
  			and r2.recycling_user_id=#{recyclingUserId,jdbcType=INTEGER}
  		</if>
  		<if test="recyclingNo != null and recyclingNo !=''">
  			and r1.recycling_no=#{recyclingNo,jdbcType=VARCHAR}
  		</if>
  		<if test="fileNo != null and fileNo !=''">
  			and r1.file_no like CONCAT(#{fileNo,jdbcType=VARCHAR}, '%')
  		</if>
  		<if test="plateNo != null and plateNo !=''">
  			and r1.plate_no like CONCAT(#{plateNo,jdbcType=VARCHAR}, '%')
  		</if>
  		<if test="organization != null and organization !=''">
  			and r1.organization=#{organization,jdbcType=VARCHAR}
  		</if>
  		<if test="staffName != null and staffName != ''">
  			s2.staff_name like CONCAT(#{staffName,jdbcType=VARCHAR}, '%')
  		</if>
  		<if test="repairShopName != null and repairShopName !=''">
  			s1.repair_shop_name like CONCAT(#{repairShopName,jdbcType=VARCHAR}, '%')
  		</if>
  		<if test="recyclingStatus != null and recyclingStatus != -1">
  			r1.recycling_status=#{recyclingStatus,jdbcType=SMALLINT}
  		</if>
  		<if test="storageStatus != null and storageStatus != -1">
  			r1.storage_status=#{storageStatus,jdbcType=SMALLINT}
  		</if>
  		<if test="dispatchStatus != null and dispatchStatus != 0"><!-- 未派单 -->
  			r1.dispatch_id=0
  		</if>
  		<if test="dispatchStatus != null and dispatchStatus != 1"><!-- 已派单 -->
  			r1.dispatch_id &gt; 0
  		</if>
  	</where>
  </select>
