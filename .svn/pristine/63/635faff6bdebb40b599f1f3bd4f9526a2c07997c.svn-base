public enum ApiResultCode {

    /**
     * 返回结果代码
     */
    NO_LOGIN(101, "没有登录或会话超时"),
    NO_AUTH(102, "没有访问权限"),
    ACCESS_TIMEOUT(103, "访问超时"),
    JWT_SIGN_ERROR(104, "签名验证不通过"),
    JWT_SIGN_EXPIRE(105, "签名过期"),


    SUCCESS(200, "success"),
    NO_DATA(201, "未查找到数据"),
    PWD_ERROR(202, "用户名或密码错误"),
    MOBILE_ERROR(203, "手机号格式错误"),
    LOGIN_CC_ERROR(204, "登录呼叫中心失败"),
    AGENT_NOT_EXIST(205, "坐席不存在"),
    EXIST_PHONE(206, "已存在手机号相同的联系人"),
    OLD_PASSWORD_ERROR(207, "原始密码错误"),

    REQUEST_PARAMETER_ERROR(300, "请求参数错误"),
    REQUEST_PARAMETER_EMPTY(301, "请求参数不能为空"),

    CUSTOMER_LOCKED(401,"客户已锁定不能认领"),
    OUT_OF_MAXNUM_RANGE(402, "已超出最大认领客户数"),

    SERVICE_ERROR(500, "哎呀，系统出错了"),

    CUSTOMIZE_ERROR(700,"自定义错误"),
    ;

    private final int code;
    private String message;

    ApiResultCode(int code, String message) {
        this.code = code;
        this.message = message;
    }

    public int getCode() {
        return code;
    }

    public String getMessage() {
        return message;
    }

    public void setMessage(String message) {
        this.message = message;
    }
}
