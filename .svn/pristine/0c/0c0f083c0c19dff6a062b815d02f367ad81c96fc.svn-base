package com.huachu.common.constants;

/**
 * @author Administrator
 * @DATE 2018/9/18
 */
public enum FileTypeEnum {

    CUSTOMER_EXCEL("客户信息excel导入", "/excel/customer"),
    CONTACT_EXCEL("联系人excel导入", "/excel/contact"),
    CALL_RESULT_IMAGE("通话结果附件上传", "/call/image");

    private String name;
    private String path;

    private FileTypeEnum(String name, String path) {
        this.name = name;
        this.path = path;
    }

    public String getName() {
        return name;
    }

    public void setName(String name) {
        this.name = name;
    }

    public String getPath() {
        return path;
    }

    public void setPath(String path) {
        this.path = path;
    }
}
